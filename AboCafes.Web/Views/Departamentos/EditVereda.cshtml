@model AboCafes.Common.Entities.Vereda

@{
    ViewData["Title"] = "Edit";
}

<h2>Editar</h2>

<h4>Vereda</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="EditVereda">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <input type="hidden" asp-for="IdCorregimiento" />

            <partial name="_Vereda" />

            <div class="form-group">
                <input type="submit" value="Guardar" class="btn btn-primary" />
                <a asp-action="DetailsCorregimiento" asp-route-id="@Model.IdCorregimiento" class="btn btn-success">Regresar a la lista</a>
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}

