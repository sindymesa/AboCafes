@model AboCafes.Common.Entities.Corregimiento


@{
    ViewData["Title"] = "Add Corregimiento";
}

<h2>Agregar</h2>

<h4>Corregimiento</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="AddCorregimiento">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="IdCiudad" />

            <partial name="_Corregimiento" />

            <div class="form-group">
                <input type="submit" value="Guardar" class="btn btn-primary" />
                <a asp-action="DetailsCiudad" asp-route-id="@Model.IdCiudad" class="btn btn-success">Regresar a la lista</a>
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}

